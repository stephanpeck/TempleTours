@{
    ViewBag.pageType = "profile-page";
    ViewBag.Title = "View Your Scheduled Tours";
    ViewBag.SubTitle = "";
    ViewBag.Background = "/img/garden.jpg";
}

@model IEnumerable<GroupInfo>


<table class="table table-striped table-hover">

    <tr>
        <th>Group Name</th>
        <th>Group Size</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Appointment Day</th>
        <th>Appointment Time</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.GroupName</td>
            <td>@x.GroupSize</td>
            <td>@x.Email</td>
            <td>@x.Phone</td>
            <td>@x.SelectedAppointmentDay</td>
            <td>@x.SelectedAppointmentTime</td>
        </tr>
    }
</table>



